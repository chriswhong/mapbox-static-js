{
  "name": "mapbox-static-js",
  "version": "2.0.0",
  "description": "A React library for creating beautiful static maps with markers, popups, and automatic interaction management",
  "type": "module",
  "main": "./dist/mapbox-static-js.umd.cjs",
  "module": "./dist/mapbox-static-js.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mapbox-static-js.js",
      "require": "./dist/mapbox-static-js.umd.cjs"
    },
    "./styles.css": "./dist/mapbox-static-js.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "tsc && vite build --config vite.lib.config.ts",
    "build:demo": "vite build --config vite.config.ts --outDir demo-dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@uiw/react-codemirror": "^4.25.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4"
  }
}
